{
    "name": "slam/flysystem-encrypted-zip-proxy",
    "type": "library",
    "description": "FlySystem adapter to store content into single encrypted zips",
    "homepage": "https://github.com/slamdunk/flysystem-encrypted-zip-proxy",
    "license": "MIT",
    "authors": [
        {
            "name": "Filippo Tessarotto",
            "email": "zoeslam@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.0",
        "ext-sodium": "*",
        "ext-zip": "*",
        "league/flysystem": "^2.3.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^v3.2.1",
        "infection/infection": "^0.25.3",
        "league/flysystem-adapter-test-utilities": "^2.2.1",
        "malukenho/mcbumpface": "^1.1.5",
        "phpunit/phpunit": "^9.5.10",
        "vimeo/psalm": "^4.11.2"
    },
    "autoload": {
        "psr-4": {
            "SlamFlysystemEncryptedZipProxy\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SlamFlysystemEncryptedZipProxyTest\\": "test/"
        }
    }
}
